pipeline {
    
agent any
    tools { 
        maven 'maven' 
        
    }
    stages {
        stage('Build') {
            steps {
            
                echo 'Building..'
                javac NewTest.java
                java NewTest
                mvn clean package ./stepdefinition
            
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}