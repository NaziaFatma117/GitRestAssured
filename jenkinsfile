pipeline {
    
agent any
    tools { 
        maven 'maven' 
        
    }
    stages {
        stage('Build') {
            steps {
            
                echo 'Building..'
                script
                {
                javac /src/test/java/stepdefinition/NewTest.java
                java /src/test/java/stepdefinition/NewTest
               }
            
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}