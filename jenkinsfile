pipeline {
    
agent any
    tools { 
        maven 'maven' 
        
    }
    stages {
        stage('Build') {
            steps {
            
                echo 'Building..'
                javac pipeline1/src/test/java/stepdefinition/NewTest.java
                java pipeline1/src/test/java/stepdefinition/NewTest
               
            
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}